            <Label Text="Choose a country you are reporting from and get linked with organization(s) that can help you." TextColor="#ffffff" FontSize="14" FontFamily="Inter-Medium"
                   HorizontalTextAlignment="Center" Margin="0,0,0,0"/>
        </StackLayout>-->

        <Frame CornerRadius="20" Padding="20,10" BackgroundColor="#EEE9ED" HasShadow="False" Grid.Row="0" Margin="20,10">
            <Label Text="Choose a country you are reporting from and get linked with organization(s) that can help you." TextColor="#646C79" FontSize="15" FontFamily="Airbnb-Mediun" HorizontalTextAlignment="Start" Margin="0,0,0,0"/>
        </Frame>

        <StackLayout Margin="20,10" Grid.Row="1">
            <StackLayout Margin="0,0,0,5" Spacing="0">

                <Label  Text="Select a Country"
                        FontFamily="Inter-Medium"
                        FontSize="16"
                        Margin="0,0,0,5"
                        TextColor="{StaticResource SecondaryColor}"/>

                <pv:PancakeView BackgroundColor="#ffffff"
                                CornerRadius="5"
                                HorizontalOptions="Fill" 
                                HeightRequest="40"
                                Padding="10,5">
                    <Grid HorizontalOptions="FillAndExpand"
                          VerticalOptions="FillAndExpand">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid HorizontalOptions="StartAndExpand"
                              VerticalOptions="CenterAndExpand">

                            <StackLayout Orientation="Vertical"
                                         VerticalOptions="CenterAndExpand">
                                <Label Text="{Binding Title}"
                                       HorizontalOptions="StartAndExpand"
                                       VerticalOptions="CenterAndExpand"
                                       FontFamily="Roboto-Medium"
                                       TextColor="{StaticResource SecondaryColor}"
                                       FontSize="16"/>

                            </StackLayout>

                        </Grid>

                        <StackLayout Grid.Column="1" VerticalOptions="CenterAndExpand" HorizontalOptions="EndAndExpand" Margin="0,0,10,0">
                            <ImageButton BackgroundColor="Transparent" Command="{Binding SelectPageCommand}" CommandParameter="{Binding .}" Source="arrowdown1.png" WidthRequest="15" 
                                         HeightRequest="15"/>
                        </StackLayout>

                    </Grid>
                    <pv:PancakeView.GestureRecognizers>
                        <TapGestureRecognizer Command="{Binding SelectPageCommand}"
                                              CommandParameter="{Binding .}"
                                              NumberOfTapsRequired="1"/>
                    </pv:PancakeView.GestureRecognizers>
                </pv:PancakeView>
